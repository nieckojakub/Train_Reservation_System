<AnchorPane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #83889C;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="train.train.ReservationConfirmationController">
   <children>
      <Text fill="#ecd0cd" layoutX="5.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You have successfully reseved ticket" textAlignment="CENTER" wrappingWidth="600.0">
