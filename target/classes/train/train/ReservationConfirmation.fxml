            <Font size="20.0" />
         </font>
      </Text>
      <ImageView fx:id="greenMarkImage" fitHeight="100.0" fitWidth="98.0" layoutX="255.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
