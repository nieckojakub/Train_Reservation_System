            <Font size="20.0" />
         </font>
      </Text>
      <Text fx:id="textMail" fill="WHITE" layoutX="1.0" layoutY="268.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ustawie@ustawie.pl" textAlignment="CENTER" wrappingWidth="607.0">
